{{>layouts/header}}


<table class="table">
  <thead>
  <tr>
    <th scope="col">#id</th>
    <th scope="col">#email</th>
    <th scope="col">#password</th>
  </tr>
  </thead>
  <tbody>
  {{#members}}
    <tr>
      <th scope="row">{{id}}</th>
      <td><a href="/members/detail/{{id}}">{{email}}</a></td>
      <td>{{password}}</td>
    </tr>
  {{/members}}
  </tbody>
</table>

{{>layouts/footer}}
